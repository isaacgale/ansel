msgstr "12 EV (exteriores)"

#: ../src/iop/filmic.c:338
msgid "13 EV (bright outdoors)"
msgstr "13 EV (exteriores luminosos)"

#: ../src/iop/filmic.c:346
msgid "14 EV (backlighting)"
msgstr "14 EV (retroiluminación)"

#: ../src/iop/filmic.c:354
msgid "15 EV (sunset)"
msgstr "15 EV (puesta de sol)"

#: ../src/iop/filmic.c:362
msgid "16 EV (HDR)"
msgstr "16 EV (HDR)"

